{
  "global": {
    // Indicators
    "isOnPage": {
      "url": "/instagram\\.com/",
    },
    "isOnMainSite": {
      "selector": "img[alt='Instagram']",
    },
    "isLoggedOut": {
      "selector": "",
      "object": { "selector": "button", "filters": [ {"match": { "innerText": "/Log in/i" }} ] },
    },
    // Actions
    "searchBar": {
      "selector": "nav input",
    },
    "notificationsReminder_disable": {
      "object": { "selector": "button", "filters": [ {"match": { "innerText": "/Not Now/i" }} ] },
    },
    // Explore
    "postLink": {
      "selector(array)": "a[href*='/p/'] > div div:nth-child(2)",
    },
    /* "postProfileLink": {
      "selector(array)": ".sound__body a.soundTitle__username span",
    }, */

  },
  "home": {
    // Indicators
    "isOnPage": {
      "url": "/instagram\\.com$/",
    },
    // Actions
    "continueAsButton": {
      "object": { "selector": "button", "filters": [ {"match": { "innerText": "/Continue as/i" }} ] },
    },
  },
  "search": {
    // Indicators
    "isOnPage": {
      "url": "/instagram\\.com\\/explore/",
    },
    // Actions

  },
  "profile": {
    // Indicators
    "isOnPage": {
      "selector": ".nZSzR h2, .nZSzR h1", // Good for testing loaded
    },
    // Actions
    "followButton": {
      "object": {"selector": "button", "filters": [ {"match": { "innerText": "/Follow$|Follow back/i" }} ], index: 0 },
    },
    // Stats
    "followerCount": {
      "object": {"selector": "a", "filters": [ {"match": { "innerText": "/ followers$| follower$/i" }} ], index: 0 },
    },
    "followingCount": {
      "object": {"selector": "a", "filters": [ {"match": { "innerText": "/ following$/i" }} ], index: 0 },
    },
    "postCount": {
      "object": {"selector": "span", "filters": [ {"match": { "innerText": "/ posts$| post$/i" }} ], index: 0 },
    },
    // Explore
    "viewFollowers": {
      "selector": "$get(profile.followerCount.selector)",
    },
    "viewFollowing": {
      "selector": "$get(profile.followingCount.selector)",
    },

  },
  "profile_followers": {
    // Indicators
    "isOnPage": {
      "url": "/\\/followers/",
    },
    "isEmpty": {
      "selector": "",
    },
    // Explore
    "profileLink": { // Good for testing loaded
      "selector(array)": "div[role='presentation'] a.notranslate",
    },
  },
  "profile_following": {
    // Indicators
    "isOnPage": {
      "url": "/\\/following/",
    },
    "isEmpty": {
      "selector": "",
    },
    // Explore
    "profileLink": {
      "selector(array)": "div[role='presentation'] a.notranslate",
    },
  },
  "post": {
    // Indicators
    "isOnPage": {
      "url": "(instagram.com\\/p\\/.*?)($|\\/$|\\/|\\?|\\/\\?|\\#|\\/\\#)",
    },
    // Actions
    "likeButton": {
      "selector": ".listenEngagement button[aria-label*='Like']",
      "object": {"selector": "button[aria-label*='Like']", "filters": [{"match": {"innerText": "/Like/"}}]},
    },
    "repostButton": {
      "selector": ".listenEngagement button[aria-label*='Repost']",
      "object": {"selector": "button[aria-label*='Repost']", "filters": [{"match": {"innerText": "/Repost/"}}]},
    },
    "followButton": {
      "selector": "button[aria-label*='Follow']",
      "object": {"selector": "button[aria-label*='Follow']", "filters": [{"match": {"innerText": "/Follow/"}}]},
    },
    "shareButton": { // Good indicator for isLoaded
      "selector": "button[aria-label*='Share']",
      "object": {"selector": "button[aria-label*='Share']", "filters": [{"match": {"innerText": "/Share/"}}]},
    },
    "disableRepostReminder_checkButton": {
      "selector": "input[name='dontRemind']",
    },
    "disableRepostReminder_closeButton": {
      "selector": "button.shareRepostedAudible__closeDialogButton",
    },
    // Stats
    "likeCount": {
      "selector": ".listenEngagement .sc-ministats-likes span[aria-hidden='true']",
    },
    "repostCount": {
      "selector": ".listenEngagement .sc-ministats-reposts span[aria-hidden='true']",
    },
    "followerCount": {
      "selector": ".sc-ministats-followers span[aria-hidden='true']",
    },
    "postDate": {
      "selector": ".fullHero__uploadTime .relativeTime",
    },
    // Explore
    "commentProfileLink": {
      "selector(array)": ".commentItem__username > a",
    },
    "relatedTrackLink": {
      "selector(array)": "a.soundTitle__title span",
    },
    "viewLikes": {
      "selector": "$get(track.likeCount.selector)",
    },
  },
  "track_likes": {
    // Indicators
    "isOnPage": {
      "url": "/\\/likes/",
    },
    "isEmpty": {
      "selector": "$get(profile_followers.isEmpty.selector)",
    },
    // Explore
    "profileLink": {
      "selector(array)": ".userBadgeListItem__title",
    },
  },
  "playlist": {
    // Indicators
    "isOnPage": {
      "url": "/\\/sets\\//",
      "selector": ".fullHero__tracksSummary",
    },
    // Explore
    "trackLink": {
      "selector(array)": "a.trackItem__trackTitle",
    },
  },
  "error": {
    // Indicators
    "spamWarning": {
      "selector": ".spamWarning",
    }
  }


}
